Algoritmo "num1 até num2"

Var
   num, num2, i: inteiro

Inicio
   escreva("Informe um número de ínicio para contagem ")
   leia(num)
   escreva("Informe um número para o final do programa ")
   leia(num2)
   para i de num ate num2 faca
      escreva(i)
      timer(100)
   fimpara
   escreval("")
   escreval("")
   escreval("Foram mostrados a sequência de ", num, " até ",num2)

Fimalgoritmo